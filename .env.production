# Production Environment Variables Template
# Copy this to .env.production and fill in your actual values

# Database (Railway will provide this)
DATABASE_URL=

# Redis (Railway will provide this)
REDIS_URL=

# Secrets (Generate strong secrets for production!)
JWT_SECRET=
COOKIE_SECRET=

# CORS Settings
STORE_CORS=https://your-storefront-domain.com
ADMIN_CORS=https://your-admin-domain.com
AUTH_CORS=https://your-admin-domain.com

# Server
PORT=9000
NODE_ENV=production

# Optional: File Storage (if using S3, MinIO, etc.)
# STORAGE_BUCKET_NAME=
# STORAGE_REGION=
# STORAGE_ACCESS_KEY_ID=
# STORAGE_SECRET_ACCESS_KEY=

